<div id="fullpage">
    <div class="section" id="section0">
    	<p>ID: {{user.id}}</p>
    	<p>Name: {{user.name}}</p>
    	<p>Instagram ID:{{user.ig_id}}</p>
    </div>
    <div class="section" id="seciton1">
    	<h1> Media Count of Your Instagram Followers using C3JS </h1>
    	<div id="c3chart"></div>
    	<script src="js/c3visualization.js"></script>
    </div>
	    <div class="section" id="section2">
    	<h1> Media Count of Your Instagram Followers </h1>
    	<div id="d3chart"></div>
    	<script src="js/visualization.js"></script>
    </div>
</div>

<script type="text/javascript" src="bower_components/fullpage.js/vendors/jquery.slimscroll.min.js"></script>
<script type="text/javascript" src="bower_components/fullpage.js/jquery.fullPage.js"></script>
<script type="text/javascript">
    $(document).ready(function() {
      $('#fullpage').fullpage({
        anchors: ['firstPage', 'secondPage', '3rdPage'],
        sectionsColor: ['#C63D0F', '#1BBC9B', '#7E8F7C'],
        navigation: true,
        navigationPosition: 'right',
        navigationTooltips: ['First page', 'Second page', 'Third and last page'],
        responsive: 900
    });
  });
</script>